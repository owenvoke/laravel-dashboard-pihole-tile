{
    "name": "owenvoke/laravel-dashboard-pihole-tile",
    "type": "library",
    "description": "A tile for Laravel Dashboard that displays statistics from Pi-hole",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\PiholeTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\PiholeTile\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "OwenVoke\\PiholeTile\\PiholeTileServiceProvider"
            ]
        }
    }
}
